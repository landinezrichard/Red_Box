/*HEADER*/

.Header
	display inline-block
	width 100%
	// height 45%
	height 444px
	background url('../images/01-index/banner.png') center no-repeat
	position relative
	
	&-logoLink
		display inline-block
		cursor pointer
		width 216px
		height 210px
		background url('../images/01-index/logo-red_box.png') center no-repeat
		position absolute
		left -25px
		top 0
	&-logo
		text-indent -9999px
		
	/*Contenedor titulo sede*/
	&-main
		display inline-block
		width 100%
		height 83px
		background url('../images/01-index/bg-head1.png') center no-repeat
		position absolute
		left 0
		bottom 0
	&-titleFondo
		display inline-block
		width 325px
		height 83px
		background url('../images/01-index/tab.png') center no-repeat
		float left
		margin-left 25px
		
	&-title
		color $white
		font-weight 400
		font-size 3.16em
		margin .5em
		
	&-sede
		display inline-block
		float right
		margin-top -100px